{
  "touch-in": {
    "tween": [
      {
        "param": "scale",
        "type": "vec2",
        "body": [
          {
            "duration": 0.25,
            "ease_func": "OutCubic",
            "start": [ 1, 1 ],
            "end":   [ 1.25, 1.25 ]
          }
        ]
      }
    ]
  },

  "moved-out": {
    "tween": [
      {
        "param": "scale",
        "type": "vec2",
        "body": [
          {
            "duration": 0.25,
            "ease_func": "OutCubic",
            "start": [ 1.25, 1.25 ],
            "end":   [ 1, 1 ]
          }
        ]
      }
    ]
  },

  "moved-in": {
    "tween": [
      {
        "param": "scale",
        "type": "vec2",
        "body": [
          {
            "duration": 0.25,
            "ease_func": "OutCubic",
            "start": [ 1, 1 ],
            "end":   [ 1.25, 1.25 ]
          }
        ]
      }
    ]
  },

  "ended-in": {
    "tween": [
      {
        "param": "scale",
        "type": "vec2",
        "body": [
          {
            "duration": 0.2,
            "ease_func": "OutBack",
            "start": [ 1.25, 1.25 ], 
            "end":   [ 1.4, 1.4 ]
          },
          {
            "duration": 0.2,
            "ease_func": "OutBack",
            "end":   [ 1, 1 ]
          }
        ]
      }
    ]
  },

  "in-from-left": {
    "tween": [
      {
        "param": "anchor_min",
        "type": "vec2",
        "body": [
          {
            "duration": 0.65,
            "ease_func": "InOutQuint",
            "copy_start": true,
            "enable_start": true,
            "start": [ -1, 0 ],
            "end":   [  0, 0 ]
          }
        ]
      },
      {
        "param": "anchor_max",
        "type": "vec2",
        "body": [
          {
            "duration": 0.65,
            "ease_func": "InOutQuint",
            "copy_start": true,
            "start": [ 0, 1 ],
            "end":   [ 1, 1 ]
          }
        ]
      }
    ]
  },

  "out-to-right": {
    "tween": [
      {
        "param": "anchor_min",
        "type": "vec2",
        "body": [
          {
            "duration": 0.65,
            "delay": 0.5,
            "disable_end": true,
            "ease_func": "InOutQuint",
            "end":   [ 1, 0 ]
          }
        ]
      },
      {
        "param": "anchor_max",
        "type": "vec2",
        "body": [
          {
            "duration": 0.65,
            "delay": 0.5,
            "ease_func": "InOutQuint",
            "end":   [ 2, 1 ]
          }
        ]
      }
    ]
  },

  "in-from-right": {
    "tween": [
      {
        "param": "anchor_min",
        "type": "vec2",
        "body": [
          {
            "duration": 0.65,
            "ease_func": "InOutQuint",
            "copy_start": true,
            "enable_start": true,
            "start": [ 1, 0 ],
            "end":   [ 0, 0 ]
          }
        ]
      },
      {
        "param": "anchor_max",
        "type": "vec2",
        "body": [
          {
            "duration": 0.65,
            "ease_func": "InOutQuint",
            "copy_start": true,
            "start": [ 2, 1 ],
            "end":   [ 1, 1 ]
          }
        ]
      }
    ]
  },

  "out-to-left": {
    "tween": [
      {
        "param": "anchor_min",
        "type": "vec2",
        "body": [
          {
            "duration": 0.65,
            "delay": 0.5,
            "ease_func": "InOutQuint",
            "disable_end": true,
            "end":   [ -1, 0 ]
          }
        ]
      },
      {
        "param": "anchor_max",
        "type": "vec2",
        "body": [
          {
            "duration": 0.65,
            "delay": 0.5,
            "ease_func": "InOutQuint",
            "end":   [ 0, 1 ]
          }
        ]
      }
    ]
  },

  "rank:touch-in": {
    "tween": [
      {
        "param": "scale",
        "type": "vec2",
        "body": [
          {
            "duration": 0.25,
            "ease_func": "OutBack",
            "end": [ 1, 1.3 ]
          }
        ]
      }
    ]
  },
  "rank:moved-out": {
    "tween": [
      {
        "param": "scale",
        "type": "vec2",
        "body": [
          {
            "duration": 0.6,
            "ease_func": "OutQuint",
            "end": [ 1, 1 ]
          }
        ]
      }
    ]
  },
  "rank:moved-in": {
    "tween": [
      {
        "param": "scale",
        "type": "vec2",
        "body": [
          {
            "duration": 0.25,
            "ease_func": "OutBack",
            "end": [ 1, 1.3 ]
          }
        ]
      }
    ]
  },
  "rank:ended-in": {
    "tween": [
      {
        "param": "scale",
        "type": "vec2",
        "body": [
          {
            "duration": 0.2,
            "ease_func": "OutBack",
            "start": [ 1, 1.3 ],
            "end": [ 1, 1.5 ]
          },
          {
            "duration": 0.3,
            "ease_func": "OutBack",
            "end": [ 1, 1 ]
          }
        ]
      }
    ]
  },

  "icon:circle": {
    "tween": [
      {
        "param": "end_angle",
        "type": "float",
        "body": [
          {
            "enable_start": true,
            "duration": 0.6,
            "ease_func": "OutQuart",
            "start": 89,
            "end":   -270
          }
        ]
      }
    ]
  },
  "icon:text": {
    "tween": [
      {
        "param": "alpha",
        "type": "float",
        "body": [
          {
            "duration": 0.4,
            "ease_func": "None",
            "start": 0,
            "end":   1
          }
        ]
      }
    ]
  }
}
