{
  "start": [
    {
      "identifier": "root",
      "tween": [
        {
          "param": "anchor_min",
          "type": "vec2",
          "body": [
            {
              "duration": 0.7,
              "copy_start": true,
              "ease_func": "OutQuint",
              "start": [ 1, 0 ],
              "end":   [ 0, 0 ]
            }
          ]
        },
        {
          "param": "anchor_max",
          "type": "vec2",
          "body": [
            {
              "duration": 0.7,
              "copy_start": true,
              "ease_func": "OutQuint",
              "start": [ 2, 1 ],
              "end":   [ 1, 1 ]
            }
          ]
        }
      ]
    }
  ],
  "end": [
    {
      "identifier": "root",
      "tween": [
        {
          "param": "anchor_min",
          "type": "vec2",
          "body": [
            {
              "duration": 0.6,
              "delay": 0.4,
              "ease_func": "InOutQuint",
              "start": [ 0, 0 ],
              "end":   [ 1, 0 ]
            }
          ]
        },
        {
          "param": "anchor_max",
          "type": "vec2",
          "body": [
            {
              "duration": 0.6,
              "delay": 0.4,
              "ease_func": "InOutQuint",
              "start": [ 1, 1 ],
              "end":   [ 2, 1 ]
            }
          ]
        }
      ]
    }
  ],

  "start-left": [
    {
      "identifier": "root",
      "tween": [
        {
          "param": "anchor_min",
          "type": "vec2",
          "body": [
            {
              "duration": 0.7,
              "delay": 0.6,
              "copy_start": true,
              "ease_func": "OutQuint",
              "start": [ -1, 0 ],
              "end":   [ 0, 0 ]
            }
          ]
        },
        {
          "param": "anchor_max",
          "type": "vec2",
          "body": [
            {
              "duration": 0.7,
              "delay": 0.6,
              "copy_start": true,
              "ease_func": "OutQuint",
              "start": [ 0, 1 ],
              "end":   [ 1, 1 ]
            }
          ]
        }
      ]
    }
  ],

  "update_score": [
    {
      "identifier": "scores",
      "tween": [
        {
          "param": "alpha",
          "type": "float",
          "body": [
            {
              "duration": 0.8,
              "end": 1
            }
          ]
        }
      ]
    }
  ]
}
