{
  "start": [
    {
      "identifier": "touch",
      "tween": [
        {
          "param": "offset",
          "type": "vec2",
          "body": [
            {
              "duration": 0.5,
              "delay": 0.1,
              "copy_start": true,
              "ease_func": "OutBack",
              "start": [ 0, -50 ],
              "end":   [ 0, 0 ]
            }
          ]
        }
      ]
    }
  ],

  "update_score": [
    {
      "identifier": "scores",
      "tween": [
        {
          "param": "alpha",
          "type": "float",
          "body": [
            {
              "duration": 0.8,
              "end": 1
            }
          ]
        }
      ]
    }
  ]
}
