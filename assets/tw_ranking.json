{
  "view-start": [
    {
      "identifier": "top10",
      "tween": [
        {
          "param": "anchor_min",
          "type": "vec2",
          "body": [
            {
              "duration": 0.7,
              "delay": 0.6,
              "disable_end": true,
              "ease_func": "InOutQuint",
              "start": [ 0, 0 ],
              "end":   [ -1, 0 ]
            }
          ]
        },
        {
          "param": "anchor_max",
          "type": "vec2",
          "body": [
            {
              "duration": 0.7,
              "delay": 0.6,
              "ease_func": "InOutQuint",
              "start": [ 1, 1 ],
              "end":   [ 0, 1 ]
            }
          ]
        }
      ]
    },
    {
      "identifier": "result",
      "tween": [
        {
          "param": "anchor_min",
          "type": "vec2",
          "body": [
            {
              "duration": 0.7,
              "delay": 0.6,
              "copy_start": true,
              "enable_start": true,
              "ease_func": "InOutQuint",
              "start": [ 1, 0 ],
              "end":   [ 0, 0 ]
            }
          ]
        },
        {
          "param": "anchor_max",
          "type": "vec2",
          "body": [
            {
              "duration": 0.7,
              "delay": 0.6,
              "copy_start": true,
              "ease_func": "InOutQuint",
              "start": [ 2, 1 ],
              "end":   [ 1, 1 ]
            }
          ]
        }
      ]
    }
  ],

  "view-end": [
    {
      "identifier": "top10",
      "tween": [
        {
          "param": "anchor_min",
          "type": "vec2",
          "body": [
            {
              "duration": 0.7,
              "delay": 0.6,
              "enable_start": true,
              "ease_func": "InOutQuint",
              "end":   [ 0, 0 ]
            }
          ]
        },
        {
          "param": "anchor_max",
          "type": "vec2",
          "body": [
            {
              "duration": 0.7,
              "delay": 0.6,
              "ease_func": "InOutQuint",
              "end":   [ 1, 1 ]
            }
          ]
        }
      ]
    },
    {
      "identifier": "result",
      "tween": [
        {
          "param": "anchor_min",
          "type": "vec2",
          "body": [
            {
              "duration": 0.7,
              "delay": 0.6,
              "disable_end": true,
              "ease_func": "InOutQuint",
              "end":   [ 1, 0 ]
            }
          ]
        },
        {
          "param": "anchor_max",
          "type": "vec2",
          "body": [
            {
              "duration": 0.7,
              "delay": 0.6,
              "ease_func": "InOutQuint",
              "end":   [ 2, 1 ]
            }
          ]
        }
      ]
    }
  ],

  "update_score": [
    {
      "identifier": "scores",
      "tween": [
        {
          "param": "alpha",
          "type": "float",
          "body": [
            {
              "duration": 0.8,
              "end": 1
            }
          ]
        }
      ]
    }
  ]
}
