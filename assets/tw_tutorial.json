{
  "start": [
    {
      "identifier": "blank:arrow",
      "tween": [
        {
          "param": "offset",
          "type": "vec2",
          "body": [
            {
              "duration": 0.3,
              "ease_func": "InCubic",
              "pingpong": true,
              "loop": true,
              "start": [ 0, 0 ],
              "end": [ 0, -4 ]
            }
          ]
        }
      ]
    },
    {
      "identifier": "blank",
      "tween": [
        {
          "param": "alpha",
          "type": "float",
          "repeat": true,
          "body": [
            {
              "duration": 0.8,
              "delay": 0.1,
              "enable_start": true,
              "ease_func": "None",
              "copy_start": true,
              "start": 0,
              "end": 1
            },
            {
              "duration": 5,
              "ease_func": "None",
              "end": 1
            },
            {
              "duration": 0.8,
              "ease_func": "None",
              "end": 0
            },
            {
              "duration": 5,
              "ease_func": "None",
              "end": 0
            }
          ]
        }
      ]
    }
  ],

  "end": [
    {
      "identifier": "blank",
      "tween": [
        {
          "param": "alpha",
          "type": "float",
          "body": [
            {
              "duration": 0.3,
              "disable_end": true,
              "ease_func": "None",
              "end": 0
            }
          ]
        }
      ]
    }
  ],

  "pause": [
    {
      "identifier": "root",
      "tween": [
        {
          "param": "alpha",
          "type": "float",
          "body": [
            {
              "duration": 0.3,
              "delay": 0.5,
              "disable_end": true,
              "ease_func": "None",
              "end": 0
            }
          ]
        }
      ]
    }
  ],

  "resume": [
    {
      "identifier": "root",
      "tween": [
        {
          "param": "alpha",
          "type": "float",
          "body": [
            {
              "duration": 0.3,
              "enable_start": true,
              "ease_func": "None",
              "end": 1
            }
          ]
        }
      ]
    }
  ],

  "like": [
    {
      "identifier": "like",
      "tween": [
        {
          "param": "alpha",
          "type": "float",
          "body": [
            {
              "duration": 0.4,
              "copy_start": true,
              "enable_start": true,
              "ease_func": "None",
              "start": 1,
              "end": 1
            },
            {
              "duration": 0.8,
              "disable_end": true,
              "ease_func": "None",
              "start": 1,
              "end": 0
            }
          ]
        },
        {
          "param": "rect",
          "type": "rect",
          "body": [
            {
              "duration": 0.8,
              "copy_start": true,
              "ease_func": "OutQuint",
              "start": [ -20, 30, 0, 30 ],
              "end": [ -20, 45, 0, 45 ]
            }
          ]
        }
      ]
    }
  ]
}
